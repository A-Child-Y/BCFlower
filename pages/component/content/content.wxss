.div{
  margin-top: 40rpx;
}
.div .title{
  position: relative;
  height: 70rpx;
  width: 90%;
  margin: 0 auto;
  line-height: 70rpx;
  padding-bottom: 15rpx;
}
.div .img{
  position: absolute;
  left: 0rpx;
  top: 13rpx;
  width: 45rpx;
  height: 45rpx;
}
.div .one{
  position: absolute;
  left: 60rpx;
  font-size: 30rpx;
  top: 3rpx;
  color: #FF6347;
}
.div .two{
  position: absolute;
  right: 10rpx;
  font-size: 25rpx;
  top: 4rpx;
  color: #515151;
}
.div .info{
  padding: 15rpx;
  background-color: #fff;
  border-radius: 20rpx;
  width: 93%;
  margin: 0 auto;
  margin-bottom: 30rpx;
}