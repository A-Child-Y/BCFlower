/* pages/meinfo/meinfo.wxss */
page{
  background-color: #f6f6f6;
}
.me-info-view{
  background-color: #fff;
}
.me-info{
  background-color: #fff;
  display: flex;
  border-bottom: 1px solid #f6f6f6;
  padding: 10rpx;
  position: relative;
  align-items: center;
  height: 100rpx;
  width: 95%;
  margin: 0 0 0 5%;
}
.me-info:nth-of-type(1){
  height: 140rpx;
}

.me-info-img{
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  position: absolute;
  right:80rpx; 
}
.me-info-img image{
  border-radius: 50%;
  width: 100%;
  height: 100%;
}
.me-info-name{
  position: absolute;
  right: 90rpx;
}
.me-info view:last-of-type{
  position: absolute;
  right: 10rpx;
  width: 50rpx;
  height: 50rpx;
}
.me-info view:last-of-type image{
  width: 100%;
  height: 100%;
}