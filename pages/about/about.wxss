/* pages/about/about.wxss */
page{
  background-color: #fff;
}
.about-one{
  height: 500rpx;
  /* background-color: #fff; */
  display: flex;
  flex-direction:column;
  justify-content: center;
  align-items: center;
}
.about-one view:nth-of-type(1){
  width: 260rpx;
  height: 240rpx;
  border-radius: 30rpx; 
}
.about-one view:nth-of-type(1) image{
  width: 100%;
  height: 100%;
  border-radius: 30rpx; 
}
.about-one view:nth-of-type(2){
  margin-top: 10rpx;
}
.about-one view:nth-of-type(3){
  margin-top: 10rpx;
  font-size: 30rpx;
}
.about-two{
  font-size: 35rpx;
  padding: 10rpx;
  text-align: center;
  margin-top: 80rpx;
}

.about-three{
  font-size: 22rpx;
  color: gray;
  text-align: center;
  margin-top: 40%;
}